local app = CLApp:new()

local testSub = app:newCommand("test")
testSub:setExecutor(function()
  print("TEST")
end)

local testSubSub = testSub:newSubCommand("sub")
testSubSub:addAlias("subbing")
testSubSub:setExecutor(function(flags,args)
  print(inspect(flags))
  print(inspect(args))
end)

app:main(function(flags,args)
  print(inspect(args))
  print(inspect(flags))
end)

app:run(...)
